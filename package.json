  "version": "1.0.0",
  "description": "api with architecture",
  "main": "index.js",
  "scripts": {
    "start": "node ./dest/main.js",
    "dev": "nodemon ./nodemon.json",
    "dev:inspect": "nodemon -w ./src -i src/**/*.spec.ts -e ts,json --exec node --inspect=localhost:9229 -r ts-node/register src/main.ts",
